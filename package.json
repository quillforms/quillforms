{
	"name": "quillforms",
	"version": "2.0.0",
	"private": true,
	"description": "Quill forms - Converstaional forms builder for WordPress",
	"author": "Mohamed Magdy <md.magdy.sa@gmail.com> (https://github.com/mdmag)",
	"license": "GPL-2.0-or-later",
	"engines": {
		"node": ">=14.0.0",
		"npm": ">=6.9.0"
	},
	"config": {
		"IS_QUILLFORMS_PLUGIN": true
	},
	"scripts": {
		"analyze-bundles": "npm run build -- --webpack-bundle-analyzer",
		"prebuild": "npm run check-engines",
		"prebuild:packages": "npm run clean:packages && lerna run build",
		"init-pot": "node init-pot.js",
		"build": "npm run build:packages && qf-scripts build",
		"build:packages": "node ./bin/packages/build.js",
		"build:package-types": "node ./bin/packages/validate-typescript-version.js && tsc --build",
		"clean": "npm run clean:packages && npm run clean:package-types",
		"clean:packages": "rimraf \"./packages/*/+(build|build-module|build-style)\"",
		"clean:package-types": "tsc --build --clean",
		"dist-w": "rimraf \"./dist\" && mkdir dist && powershell Compress-Archive -Path assets, build, dependencies, includes, lib, quillforms.php, readme.txt -DestinationPath \"./dist/quillforms.zip\"",
		"dist": "rimraf \"./dist\" && mkdir dist && zip -rq dist/quillforms.zip assets build dependencies includes lib quillforms.php readme.txt",
		"changelog": "./bin/plugin/cli.js changelog",
		"check-engines": "qf-scripts check-engines",
		"precheck-local-changes": "npm run docs:build",
		"check-local-changes": "( git diff -U0 | xargs -0 node bin/process-git-diff ) || ( echo \"There are local uncommitted changes after one or both of 'npm install' or 'npm run docs:build'!\" && exit 1 );",
		"predev": "npm run check-engines",
		"dev": "npm run build:packages && concurrently \"qf-scripts start\" \"npm run dev:packages\"",
		"dev:packages": "node ./bin/packages/watch.js",
		"presafe-clean": "node ./bin/check-critical-files.js",
		"safe-clean": "npm run clean:packages",
		"format-js": "qf-scripts format-js",
		"lint": "concurrently \"npm run lint-lockfile\" \"npm run lint-js\" \"npm run lint-pkg-json\" \"npm run lint-css\"",
		"lint-js": "qf-scripts lint-js",
		"lint-js:fix": "npm run lint-js -- --fix",
		"prelint-php": "npm run wp-env run composer install -- --no-interaction",
		"lint-php": "npm run wp-env run composer run-script lint",
		"lint-pkg-json": "qf-scripts lint-pkg-json . 'packages/*/package.json'",
		"lint-lockfile": "node ./bin/validate-package-lock.js",
		"lint-css": "qf-scripts lint-style '**/*.scss'",
		"lint-css:fix": "npm run lint-css -- --fix",
		"lint:md-js": "qf-scripts lint-md-js",
		"lint:md-docs": "qf-scripts lint-md-docs",
		"package-plugin": "./bin/build-plugin-zip.sh",
		"pot-to-php": "./bin/pot-to-php.js",
		"publish:check": "lerna updated",
		"publish:dev": "npm run build:packages && lerna publish --dist-tag next",
		"publish:patch": "npm run build:packages && lerna publish --dist-tag patch",
		"publish:prod": "npm run build:packages && lerna publish",
		"env": "qf-scripts env",
		"wp-env": "wp-env",
		"pretest-unit-php": "wp-env start",
		"test-unit-php": "wp-env run phpunit 'phpunit -c /var/www/html/wp-content/plugins/quillforms/phpunit.xml.dist --verbose'",
		"makepot-php": "php -d memory_limit=-1 C:/wp-cli/wp-cli.phar i18n make-pot . languages/quillforms.pot --domain=quillforms --exclude=\"node_modules/,vendor/,tests/,packages/,client/\" --headers='{\"Report-Msgid-Bugs-To\":\"https://github.com/quillforms/quillforms/issues\",\"X-Domain\":\"quillforms\"}'",
		"makepot-js": "wp i18n make-pot --domain=quillforms --skip-js=false --skip-php=true --include=packages/,client/ languages/quillforms-js.pot",
		"makepot": "npm run makepot-php && npm run makepot-js"
	},
